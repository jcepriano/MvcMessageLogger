@model IEnumerable<MvcMessageLogger.Models.User>

    <h1>Users</h1>
    <form method="get" action="/users/stats">
        <button type="submit">Statistics</button>
    </form>
    <br />

    @foreach(var user in Model)
    {
        <p><a href="/users/@user.Id">Login</a> Name: @user.Name, Username: @user.Username</p>
    }

    <a class="profileLink" href="/users/new">Create a New Profile</a>